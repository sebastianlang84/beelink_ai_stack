# mcp-owui-connector Config (Non-Secrets)
OWUI_CONNECTOR_BIND_ADDRESS=127.0.0.1
OWUI_CONNECTOR_HOST_PORT=8877

# Docker-internal URL of Open WebUI (same ai-stack network)
OPEN_WEBUI_BASE_URL=http://owui:8080

# Safety switches (default: read-only)
OWUI_CONNECTOR_ALLOW_KNOWLEDGE_WRITE=false
OWUI_CONNECTOR_ALLOW_ADMIN_WRITE=false

# Optional: where repo tool-import JSONs are mounted inside the container
OWUI_CONNECTOR_IMPORTS_DIR=/imports

